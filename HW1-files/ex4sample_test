.global _start

.section .text

  mov $result, %rax
  cmpl $1, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
Node1: 
	.quad 0
	.int 10
	.quad Node2
Node2: 
	.quad Node1
	.int 20
	.quad Node3
Node3: 
	.quad Node2
	.int 30
	.quad 0

node: .quad Node2
result: .byte 0
