.global _start

.section .text

  mov $BitCheck, %rax
  cmpl $1, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
Num1: .int 0b11000001000010100000000001010111
Num2: .int 0b11110001011000000110001000000000
BitCheck: .byte 0
